{
  "HelloController": {
    "_controllerName": "HelloController",
    "_prefix": "hello",
    "_handlers": {
      "getHello": {
        "path": "greeting",
        "httpMethod": "GET"
      },
      "getStreamingHello": {
        "path": "streaming",
        "httpMethod": "GET"
      }
    }
  },
  "workers": {
    "HelloWorkerService": {
      "_workerName": "HelloWorkerService",
      "_handlers": {
        "calculatePi": {
          "isGenerator": true
        }
      }
    }
  }
}